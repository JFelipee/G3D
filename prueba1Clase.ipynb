    "glutInit(sys.argv)\n",
    "def torus():\n",
    "    glutWireTorus(0.25, 0.6, 10, 10)\n",
    "    glFlush()\n",
    "\n",
    "glutCreateWindow(\"Mi primera ventana\")\n",
    "glutDisplayFunc(torus)\n",
    "glutMainLoop\n"
